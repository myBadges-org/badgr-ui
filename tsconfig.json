{
	"compileOnSave": false,
	"compilerOptions": {
		"esModuleInterop": true,
		"importHelpers": true,
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"downlevelIteration": true,
		"noImplicitAny": false,
		"target": "ES2022",
		"types": ["node"],
		"typeRoots": ["node_modules/@types"],
		"lib": ["es7", "es6", "dom", "esnext"],
		"module": "commonjs",
		"baseUrl": "./",
		"useDefineForClassFields": false
	}
}
